<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="516.0" prefWidth="648.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Baccara.Controller.BaccaraMenuViewController">
   <children>
      <ImageView fitHeight="516.0" fitWidth="794.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/tableBackground.jpg" />
         </image>
      </ImageView>
      <Button fx:id="baccaraButton" layoutX="180.0" layoutY="322.0" mnemonicParsing="false" onAction="#startBaccara" prefHeight="55.0" prefWidth="289.0" text="Play Baccara" />
      <ImageView fx:id="spadeIcon" fitHeight="123.0" fitWidth="146.0" layoutX="44.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/heartpokerIcon.png" />
         </image>
      </ImageView>
      <Button fx:id="backToMenuButton" layoutX="543.0" layoutY="21.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="27.0" prefWidth="84.0" text="Go back" />
   </children>
</AnchorPane>
