<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="817.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Roulette.MainMenuRouletteFXMLController">
   <children>
      <ImageView fitHeight="439.0" fitWidth="972.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Roulette/vegas.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="rouletteLogo" fitHeight="235.0" fitWidth="329.0" layoutX="322.0" layoutY="77.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Roulette/vegasRoulette.png" />
         </image>
      </ImageView>
      <Button fx:id="rouletteStartButton" layoutX="629.0" layoutY="292.0" mnemonicParsing="false" onAction="#rouletteStart" prefHeight="72.0" prefWidth="329.0" text="Start Roulette">
         <font>
            <Font name="Lucida Calligraphy Italic" size="35.0" />
         </font></Button>
      <Button layoutX="14.0" layoutY="292.0" mnemonicParsing="false" onAction="#endGame" prefHeight="79.0" prefWidth="329.0" text="Go back">
         <font>
            <Font name="Lucida Calligraphy Italic" size="35.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
